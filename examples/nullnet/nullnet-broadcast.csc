    </plugin_config>
    <width>1858</width>
    <z>0</z>
    <height>166</height>
    <location_x>9</location_x>
    <location_y>723</location_y>
  </plugin>
  <plugin>
    org.contikios.cooja.plugins.RadioLogger
    <plugin_config>
      <split>150</split>
      <formatted_time />
      <showdups>false</showdups>
      <hidenodests>false</hidenodests>
    </plugin_config>
    <width>500</width>
    <z>2</z>
    <height>300</height>
